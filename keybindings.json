[
	{
		"key": "ctrl+escape",
		"command": "workbench.action.terminal.toggleTerminal",
		"when": "terminal.active"
	},
	{
		"key": "cmd+b",
		"command": "editor.action.revealDefinition",
		"when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEdito"
	},
	{
		"key": "a",
		"command": "explorer.newFile",
		"when": "filesExplorerFocus && !inputFocus"
	},
	{
		"key": "shift+a",
		"command": "explorer.newFolder",
		"when": "filesExplorerFocus && !inputFocus"
	},
	{
		"key": "alt+enter",
		"command": "editor.action.quickFix",
		"when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
	},
	{
		"key": "alt+enter",
		"command": "workbench.action.terminal.quickFix",
		"when": "terminalFocus && terminalProcessSupported"
	},
	{
		"key": "alt+enter",
		"command": "problems.action.showQuickFixes",
		"when": "problemFocus"
	},
	{
		// "key": "cmd+shift+e", // when Explorer not open
		"key": "shift+alt+e",
		"command": "workbench.files.action.focusFilesExplorer",
		"when": "!explorerViewletVisible"
	},
	{
		// "key": "cmd+shift+e", // when Explorer open
		"key": "shift+alt+e",
		"command": "workbench.action.toggleSidebarVisibility",
		"when": "explorerViewletVisible"
	},
	{
		"key": "cmd+k cmd+b",
		"command": "-workbench.action.toggleSidebarVisibility"
	},
	{
		// "key": "shift+cmd+ctrl+e",
		"key": "cmd+shift+e", // when Explorer open
		"command": "workbench.view.explorer"
	},
	{
		"key": "shift+cmd+ctrl+s",
		"command": "workbench.action.toggleStatusbarVisibility"
	},
	{
		"key": "ctrl+tab",
		"command": "-workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup",
		"when": "!activeEditorGroupEmpty"
	},
	{
		"key": "ctrl+tab",
		"command": "-workbench.action.quickOpenNavigateNextInEditorPicker",
		"when": "inEditorsPicker && inQuickOpen"
	},
	{
		"key": "ctrl+tab",
		"command": "workbench.action.nextEditor"
	},
	{
		"key": "shift+cmd+]",
		"command": "-workbench.action.nextEditor"
	},
	{
		"key": "ctrl+shift+tab",
		"command": "workbench.action.previousEditor"
	},
	{
		"key": "shift+cmd+[",
		"command": "-workbench.action.previousEditor"
	},
	{
		"key": "ctrl+shift+tab",
		"command": "-workbench.action.quickOpenLeastRecentlyUsedEditorInGroup",
		"when": "!activeEditorGroupEmpty"
	},
	{
		"key": "ctrl+shift+tab",
		"command": "-workbench.action.quickOpenNavigatePreviousInEditorPicker",
		"when": "inEditorsPicker && inQuickOpen"
	},
	{
		"key": "pageup pageup",
		"command": "editor.action.rename",
		"when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
	},
	{
		"key": "delete delete",
		"command": "editor.action.deleteLines",
		"when": "textInputFocus && !editorReadonly"
	},
	{
		"key": "cmd+.",
		"command": "breadcrumbs.focusAndSelect"
	},

	{
		"key": "shift+cmd+g",
		"command": "workbench.view.scm",
		"when": "workbench.scm.active"
	},
	{
		"key": "ctrl+shift+g",
		"command": "-workbench.view.scm",
		"when": "workbench.scm.active"
	},
	{
		"key": "shift+cmd+b",
		"command": "editor.action.goToTypeDefinition"
	},
	{
		"key": "shift+cmd+c",
		"command": "list.collapseAll",
		"when": "listFocus && !inputFocus && !treestickyScrollFocused"
	},
	{
		"key": "shift+cmd+up",
		"command": "-list.All",
		"when": "listFocus && !inputFocus && !treestickyScrollFocused"
	},
	{
		"key": "cmd+b",
		"command": "-workbench.action.toggleSidebarVisibility"
	},
	{
		"key": "alt+s",
		"command": "workbench.action.gotoLine"
	},
	// {
	// 	"key": "alt+shift+s",
	// 	"command": "workbench.action.gotoSymbol"
	// },
	{
		"key": "cmd+shift+c",
		"command": "search.action.collapseSearchResults",
		"when": "searchViewletVisible"
	},
	{
		"key": "cmd+shift+x",
		"command": "search.action.expandSearchResults",
		"when": "searchViewletVisible"
	},
	{
		"key": "cmd+shift+f",
		"command": "search.action.focusSearchList",
		"when": "searchViewletFocus"
	},
	{
		"key": "cmd+m",
		"command": "editor.emmet.action.wrapWithAbbreviation",
		"when": "editorFocus"
	},
	{
		"key": "cmd+m cmd+m",
		"command": "editor.emmet.action.matchTag",
		"when": "editorFocus"
	},
	{
		"key": "cmd+[",
		"command": "editor.fold",
		"when": "editorFocus && foldingEnabled"
	},
	{
		"key": "cmd+]",
		"command": "editor.unfold",
		"when": "editorFocus && foldingEnabled"
	},
	{
		"key": "ctrl+alt+cmd+[",
		"command": "editor.foldAll",
		"when": "editorFocus && foldingEnabled"
	},
	{
		"key": "ctrl+alt+cmd+]",
		"command": "editor.unfoldAll",
		"when": "editorFocus && foldingEnabled"
	},
	{
		"key": "alt+cmd+[",
		"command": "editor.foldRecursively",
		"when": "editorFocus && foldingEnabled"
	},
	{
		"key": "alt+cmd+]",
		"command": "editor.unfoldRecursively",
		"when": "editorFocus && foldingEnabled"
	},
	// {
	// 	"key": "ctrl+`",
	// 	"command": "workbench.action.toggleMaximizedPanel"
	// },
	{
		"key": "ctrl+`",
		"command": "editor.action.showHover"
	}
]
